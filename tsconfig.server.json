{
    "extends": "./tsconfig.node.json",
    "compilerOptions": {
        "lib": [
            "ES2022"
        ],
        "module": "NodeNext",
        "moduleResolution": "NodeNext",
        "target": "ES2022",
        "noEmit": true,
        "esModuleInterop": true,
        "strict": true,
        "verbatimModuleSyntax": false
    },
    "include": [
        "api/**/*.ts"
    ]
}